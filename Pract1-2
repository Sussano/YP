#include <iostream>
#include <string>
using namespace std;
int i = 0;
int main()
{
    string str1 = " верблюд";
    string str2 = "a";
    string str3 = "ов";
    string result;
    cout << "Введите число, которое принадледит диапазану от 1 до 100: \n";
    cin >> i;
    if (i >= 1 && i <= 100){
        cout << "В караване ";
        cout << i;
        if (i % 10 == 1 and i!= 11){
            result = str1;
            cout << result <<endl;
            }else if ((i % 10 ==2 or i % 10 == 3 or i % 10 == 4 ) and (i != 14) and (i != 13) and (i != 12)) {
                result = str1 + str2;
                cout << result << endl;
                }else {
                    result = str1 + str3;
                    cout << result << endl;
                    }
        }else {
            cout << "Число не принадлежит диапазону!";
            }
    
    return 0;
}
